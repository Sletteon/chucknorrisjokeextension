{
  "name": "Háttérszínlilító",
  "description": "This extension will make your webpage purple.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_icon": "icon_128.png"
  },
  "permissions":
  [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "manifest_version": 2
} 