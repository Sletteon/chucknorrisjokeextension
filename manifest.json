{
  "name": "Chuck Norris Joke",
  "description": "This extension will tell you a Chuck Norris Joke is press the button in the top right corner. Creators: sletteon, kéksóskajóska",
  "version": "1.7",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "options_page": "popup.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_icon": "chuck norris.png",
      "default_popup": "popup.html"
  },
  "permissions":
  [
    "<all_urls>",
    "activeTab"
  ],
  "manifest_version": 2
} 
